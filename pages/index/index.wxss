@import "../../app.wxss";
 .weui-grid{width: 25%;border-right: 0;border-bottom: 0;} 
image{width: 100%;height: 100%;}
.banner-img image{display: block;}
swiper{min-height: 180px;}
.app-wrapper{padding-bottom:0;}
.app-wrapper .page__bd{position: relative;}
.app-wrapper .page__bd .searchbar-result{position: absolute;}
/* .index-recommend-cat image{width: 50px;height: 50px;} */
#count_timer image , .title-img image{width: 16px;height: 16px;}
#count_timer view{display: inline-block;}
.theme-title image{width: 16px;height: 16px;}
#count_timer .time{background-color:#ff4e44;color: #fff;padding:0 3px;}
page__bd{position: relative;}
#searchResult{position: absolute;top:45px;left: 0;width: 100%;margin:0 auto;padding: 0 7.5px;box-sizing: border-box;background-color:#fff;}
#searchResult navigator{display: inline-block;width: 100%;padding-left:45px;line-height:30px;box-sizing: border-box;padding-right: 45px;}
#searchResult navigator text{display: inline-block;width: 100%;border-bottom: 1px solid #e5e5e5;font-size: 14px;color: #666;}
.notice .weui-cell__hd view {
font-family:'DINSchrift';
font-size:24px;
font-weight:bold;
padding-right:15px;
border-right:1px solid #333;
}
.page__bd .weui-search-bar{background-color:#000;}
.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result:before{
    display: none;
}


/* 资讯字体 */
@font-face {
    font-family: 'DINSchrift';
    src: url('DINEngschrift.eot');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'DINSchrift';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('DINEngschrift.ttf') format('truetype'),
        url('DINEngschrift.svg#DINEngschrift') format('svg');
    font-weight: normal;
    font-style: normal;
}